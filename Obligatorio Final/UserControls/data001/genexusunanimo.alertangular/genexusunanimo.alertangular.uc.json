			"Name": "Message",
			"Type": "string",
			"Default": ""
		} ,
		{
			"Id": "Position",
			"Name": "Position",
			"Type": "enum",
			"Default": ""
		} ,
		{
			"Id": "ShowMultiple",
			"Name": "ShowMultiple",
			"Type": "boolean",
			"Default": "false"
		} ,
		{
			"Id": "Count",
			"Name": "Count",
			"Type": "numeric",
			"Default": "0"
		} 
	],
