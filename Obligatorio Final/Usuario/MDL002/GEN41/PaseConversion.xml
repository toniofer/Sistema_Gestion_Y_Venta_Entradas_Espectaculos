<Statements>
<Statement>CREATE VIEW [Pase] AS SELECT PaseId, PaseTipo, EspectaculoId, SectorId, PaisCompraPaseId, NULL AS NombreInvitado FROM (SELECT [EntradaId] AS PaseId, 'Entrada' AS PaseTipo, [EspectaculoId] AS EspectaculoId, [SectorId] AS SectorId, [PaisCompraId] AS PaisCompraPaseId FROM [Entrada]) SV UNION SELECT PaseId, PaseTipo, EspectaculoId, NULL AS SectorId, NULL AS PaisCompraPaseId, NombreInvitado FROM (SELECT [InvitacionId] AS PaseId, 'Invitacion' AS PaseTipo, [EspectaculoId] AS EspectaculoId, [InvitacionNombreInvitado] AS NombreInvitado FROM [Invitacion]) SV</Statement></Statements>
